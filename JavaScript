// 3. åŸ·è¡Œè‡ªå‹•å¹³é‹ªé‚è¼¯ï¼ˆé€™è¦åœ¨ç‰©ä»¶å®šç¾©ä¹‹å¤–è·‘ï¼‰
balloonUrls.forEach((url, index) => {
    const id = `ball_item_${index}`;
    photoLibrary[id] = {
        tag: "æ°£çƒä½ˆç½®",
        title: `ä½ˆç½®æˆå“ ${index + 1}`,
        loc: "ç²¾é¸æ¡ˆä¾‹",
        images: [url],
        desc: "å…«æ¯…æ´»å‹•å·¥ç¨‹ç²¾é¸ä½ˆç½®ä½œå“ã€‚"
    };
    // è‡ªå‹•å¡å…¥åˆ†é¡é™£åˆ—
    photos_Decor.push(id);
});


// --- åœ–ç‰‡åŠ é€Ÿé è¼‰åŠŸèƒ½ (æ”¾åœ¨ photoLibrary å®šç¾©ä¹‹å¾Œ) ---
(function() {
    console.log("ğŸš€ å•Ÿå‹•åœ–ç‰‡åŠ é€Ÿé è¼‰...");

    const allData = Object.values(photoLibrary);
    
    // 1. å„ªå…ˆé è¼‰ï¼šæ¯å€‹æ¡ˆä¾‹çš„ç¬¬ä¸€å¼µåœ–ï¼ˆè®“ç¸®åœ–ç§’å‡ºï¼‰
    allData.forEach(data => {
        if (data.images && data.images.length > 0) {
            const firstImg = new Image();
            firstImg.src = data.images[0];
        }
    });

    // 2. é–’ç½®é è¼‰ï¼šç•¶ç€è¦½å™¨æœ‰ç©ºæ™‚ï¼Œä¸‹è¼‰å‰©ä¸‹çš„åœ–ç‰‡
    window.addEventListener('load', () => {
        setTimeout(() => {
            allData.forEach(data => {
                if (data.images && data.images.length > 1) {
                    for (let i = 1; i < data.images.length; i++) {
                        const nextImg = new Image();
                        nextImg.src = data.images[i];
                    }
                }
            });
            console.log("âœ… æ‰€æœ‰åœ–ç‰‡ç·©å­˜æŒ‡ä»¤å·²ç™¼é€");
        }, 2000); // ç¶²é é–‹å•Ÿ 2 ç§’å¾Œæ‰é–‹å§‹æŠ“å‰©ä¸‹çš„ï¼Œä¸æ¶é »å¯¬
    });
})();

  
// ==========================================
// 2. å·¥å…·å‡½å¼
// ==========================================
let currentProject = null;
let currentImgIdx = 0;

function switchModel(productId, element) {
    const viewer = document.getElementById('main-viewer');
    const title = document.getElementById('title-table');
    const specs = document.getElementById('specs-table');
    const btns = document.querySelectorAll('#model-list button');
    const data = modelData[productId];
    if (data && viewer) {
        viewer.src = data.src;
        title.innerText = data.title;
        specs.innerHTML = data.specs;
    }
    if (element) {
        btns.forEach(btn => btn.classList.remove('active'));
        element.classList.add('active');
    }
}

function loadGallery(id, list) {
    const el = document.getElementById(id);
    if (!el || !list) return;
    el.innerHTML = list.map(projectId => {
        const info = photoLibrary[projectId];
        if (!info) return '';
        const coverUrl = info.images[0];
        return `
            <div class="col-6 col-md-4 mb-4">
                <div class="album-img-wrapper shadow-sm" onclick="openProjectGallery('${projectId}')">
                    <span class="photo-tag">${info.tag} (${info.images.length}å¼µ)</span>
                    <img src="${coverUrl}" class="album-img" loading="lazy">
                </div>
            </div>`;
    }).join('');
}

function openProjectGallery(projectId) {
    currentProject = photoLibrary[projectId];
    if (!currentProject) return;
    currentImgIdx = 0;
    updateLightbox();
}
function updateLightbox() {
    const lb = document.getElementById('imgLightbox');
    const info = currentProject;
    const url = info.images[currentImgIdx];
    const formattedDesc = info.desc.replace(/\n/g, '<br>');

    lb.innerHTML = `
        <div class="lightbox-content" onclick="event.stopPropagation()">
            <button class="close-btn" onclick="closeLightbox()">âœ•</button>
            
            <div class="lightbox-image-side">
                <img src="${url}" id="target-img" class="photo-fade">
                
                ${info.images.length > 1 ? `
                    <div class="nav-controls">
                        <button class="nav-btn prev" onclick="event.stopPropagation(); changeImage(-1)">â®</button>
                        <button class="nav-btn next" onclick="event.stopPropagation(); changeImage(1)">â¯</button>
                    </div>
                    <div class="img-counter">
                        <span class="current">${currentImgIdx + 1}</span>
                        <span class="divider">/</span>
                        <span class="total">${info.images.length}</span>
                    </div>
                ` : ''}
            </div>
            
            <div class="lightbox-info-side">
                <h2 class="brand-red-text">${info.title}</h2>
                <p class="location-tag"><i class="bi bi-geo-alt-fill me-1"></i>${info.loc}</p>
                <div class="divider"></div>
                <div class="info-body">
                    <p>${formattedDesc}</p>
                </div>
                <div style="margin-top: auto;">
                    <button class="btn btn-outline-light btn-sm w-100" onclick="closeLightbox()">è¿”å›ç›¸ç°¿</button>
                </div>
            </div>
        </div>`;
    
    lb.style.display = 'flex';
    document.body.style.overflow = 'hidden';
     setTimeout(initSwipe, 100);
}

function closeLightbox() {
    const lb = document.getElementById('imgLightbox');
    lb.style.display = 'none';
    // æ¢å¾©èƒŒæ™¯æ²å‹•
    document.body.style.overflow = 'auto';
}


function changeImage(step) {
    if (!currentProject) return;

    const imgElement = document.getElementById('target-img');
    const counterElement = document.querySelector('.img-counter .current');

    // 1. å…ˆè®“ç…§ç‰‡æ·¡å‡º (è®Šé€æ˜)
    imgElement.classList.add('photo-hidden');

    // 2. ç­‰æ·¡å‡ºå‹•ç•«è·‘å®Œ (ç´„ 300ms) å†æ›åœ–
    setTimeout(() => {
        // æ›´æ–°ç´¢å¼•
        currentImgIdx += step;
        if (currentImgIdx < 0) currentImgIdx = currentProject.images.length - 1;
        if (currentImgIdx >= currentProject.images.length) currentImgIdx = 0;

        const newSrc = currentProject.images[currentImgIdx];

        // æ›´æ›åœ–ç‰‡è·¯å¾‘
        imgElement.src = newSrc;

        // ç•¶åœ–ç‰‡ã€Œè¼‰å…¥å®Œæˆã€å¾Œï¼Œå†æ·¡å…¥
        imgElement.onload = () => {
            imgElement.classList.remove('photo-hidden');
            if (counterElement) {
                counterElement.innerText = currentImgIdx + 1;
            }
        };
        
        // å¦‚æœåœ–ç‰‡å·²ç¶“åœ¨å¿«å–ä¸­ï¼Œç›´æ¥æ·¡å…¥
        if (imgElement.complete) {
            imgElement.classList.remove('photo-hidden');
        }
    }, 300); 
}

// ==========================================
// 3. æ ¸å¿ƒåŸ·è¡Œå€
// ==========================================
window.onload = function() {
    loadGallery('gallery-1', photos_Tables);
    loadGallery('gallery-2', photos_Truss);
    loadGallery('gallery-3', photos_Tent);
    loadGallery('gallery-4', photos_Stage);
    loadGallery('gallery-5', photos_Decor);

    const loader = document.getElementById('loading-screen');
    if(loader) {
        setTimeout(() => {
            loader.style.opacity = '0';
            setTimeout(() => { loader.style.display = 'none'; }, 800);
        }, 1500); 
    }
};

let touchStartX = 0;
let touchEndX = 0;
let isDragging = false;

// åˆå§‹åŒ–æ»‘å‹•åŠŸèƒ½ (å»ºè­°åœ¨é é¢è¼‰å…¥æˆ– updateLightbox å¾ŒåŸ·è¡Œ)
function initSwipe() {
    const slideArea = document.getElementById('target-img');
    if (!slideArea) return;

    // --- æ‰‹æ©Ÿè§¸æ§äº‹ä»¶ ---
    slideArea.addEventListener('touchstart', e => {
        touchStartX = e.changedTouches[0].screenX;
    }, { passive: true });

    slideArea.addEventListener('touchend', e => {
        touchEndX = e.changedTouches[0].screenX;
        handleSwipe();
    }, { passive: true });

    // --- é›»è…¦æ»‘é¼ äº‹ä»¶ ---
    slideArea.addEventListener('mousedown', e => {
        touchStartX = e.screenX;
        isDragging = true;
        e.preventDefault(); // é˜²æ­¢æ»‘é¼ æ‹–æ›³åœ–ç‰‡çš„é è¨­è¡Œç‚º
    });

    slideArea.addEventListener('mouseup', e => {
        if (!isDragging) return;
        touchEndX = e.screenX;
        isDragging = false;
        handleSwipe();
    });

    // é›¢é–‹åœ–ç‰‡ç¯„åœæ™‚å–æ¶ˆæ‹–æ›³ç‹€æ…‹
    slideArea.addEventListener('mouseleave', () => { isDragging = false; });
}

function handleSwipe() {
    const swipeThreshold = 50; // æ»‘å‹•è¶…é 50px æ‰è§¸ç™¼æ›åœ–
    const diff = touchStartX - touchEndX;

    if (Math.abs(diff) > swipeThreshold) {
        if (diff > 0) {
            // å‘å·¦æ»‘ -> ä¸‹ä¸€å¼µ
            changeImage(1);
        } else {
            // å‘å³æ»‘ -> ä¸Šä¸€å¼µ
            changeImage(-1);
        }
    }
}

// 1. åˆ‡æ›é¡¯ç¤ºæ¨¡å¼
function toggleMonitor(type) {
    if (type === 'real') {
        // é»æ“Š LIVE å¯¦å¢ƒæ™‚ï¼Œé¡¯ç¤ºå¯†ç¢¼é®ç½©
        const overlay = document.getElementById('auth-overlay');
        overlay.style.opacity = '1';
        overlay.style.pointerEvents = 'auto';
        
        // è‡ªå‹•èšç„¦è¼¸å…¥æ¡†
        setTimeout(() => document.getElementById('access-code').focus(), 500);
    } else {
        // é»æ“Š 3D å¯¦å¢ƒï¼Œéš±è—é®ç½©ä¸¦é¡¯ç¤ºé€²åº¦é¢æ¿
        hideAuth();
        document.getElementById('progress-panel').style.display = 'none';
        // é€™è£¡å¯ä»¥æ”¾åˆ‡æ›å› 3D çš„é‚è¼¯
    }
}

// 2. é©—è­‰å¯†ç¢¼
function checkAccess() {
    const code = document.getElementById('access-code').value;
    const errorMsg = document.getElementById('auth-error');
    const panel = document.getElementById('progress-panel');
    
    // è¨­å®šä½ çš„å¯†ç¢¼ï¼ˆæ‰‹æ©Ÿè™Ÿç¢¼ï¼‰
    if (code === "0912345678") { 
        hideAuth(); // é©—è­‰æˆåŠŸï¼Œéš±è—é®ç½©
        panel.style.display = 'block'; // é¡¯ç¤ºåŸæœ¬éš±è—çš„é€²åº¦æ¢
        errorMsg.style.display = 'none';
        
        // è®“æŒ‰éˆ•åˆ‡æ›é¡è‰²
        document.getElementById('btn-real').classList.replace('btn-dark', 'btn-danger');
        document.getElementById('btn-3d').classList.replace('btn-danger', 'btn-dark');
    } else {
        errorMsg.style.display = 'block';
        document.getElementById('access-code').value = '';
    }
}

// 3. éš±è—é®ç½©
function hideAuth() {
    const overlay = document.getElementById('auth-overlay');
    overlay.style.opacity = '0';
    overlay.style.pointerEvents = 'none';
    document.getElementById('auth-error').style.display = 'none';
    document.getElementById('access-code').value = '';
}

</script>
